  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/webgl/Shoes1.jsx", "src/webgl/Shoes2.jsx", "src/webgl/Shoes14.jsx", "src/webgl/Shoes12.jsx", "src/webgl/Shoes10.jsx"],
  "exclude": ["node_modules"]
}
